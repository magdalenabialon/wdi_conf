<script>

  $('#ticketPurchaseButton').on('click', function() {

    $('.quizmodal-overlay').css('display', 'flex');
        console.log('calling run intro');
        runIntro();
  })



  $('.timetable-speaker-open').on('click', function(event) {
    var clickedTimetableElem = $(event.target);
    var elemToHide = clickedTimetableElem.closest( $('.t-info') ).find( $('.t-info-bottom') );

    if ( elemToHide.hasClass('t-closed') ) {
      elemToHide.removeClass('t-closed');
      elemToHide.addClass('t-open');
    } else {
      elemToHide.removeClass('t-open');
      elemToHide.addClass('t-closed');
    }

  })

</script>
